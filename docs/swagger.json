{
    "swagger": "2.0",
    "info": {
        "description": "MiCasaのAPIドキュメント",
        "title": "MiCasa Main API",
        "contact": {
            "name": "Sw-Saturn"
        },
        "license": {
            "name": "MIT License",
            "url": "https://github.com/MiCasa-IoT/MainAPI/blob/master/LICENSE"
        },
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/api/v1/db/document/create": {
            "post": {
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "新規のドキュメントを作成する",
                "parameters": [
                    {
                        "description": "User",
                        "name": "create",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.User"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.InsertOneResult"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/models.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/api/v1/db/document/delete": {
            "delete": {
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "既存のドキュメントを削除する",
                "parameters": [
                    {
                        "description": "UserID",
                        "name": "delete",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.User"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.DeleteResult"
                        }
                    },
                    "400": {
                        "description": "User Not found",
                        "schema": {
                            "$ref": "#/definitions/models.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/models.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/api/v1/db/document/read": {
            "post": {
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "指定したIDのドキュメントを取得する",
                "parameters": [
                    {
                        "description": "UserID",
                        "name": "read",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.User"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.User"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/models.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/api/v1/db/document/readall": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "summary": "全てのドキュメントを取得する",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/models.User"
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/models.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/api/v1/db/document/update": {
            "post": {
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "既存のドキュメントを更新する",
                "parameters": [
                    {
                        "description": "UserID",
                        "name": "read",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.User"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.UpdateResult"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/models.ErrorResponse"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "models.DeleteResult": {
            "type": "object",
            "properties": {
                "deletedCount": {
                    "description": "The number of documents deleted.",
                    "type": "integer"
                }
            }
        },
        "models.ErrorResponse": {
            "type": "object",
            "properties": {
                "status": {
                    "type": "string"
                }
            }
        },
        "models.InsertOneResult": {
            "type": "object",
            "properties": {
                "insertedID": {
                    "description": "The _id of the inserted document. A value generated by the driver will be of type primitive.ObjectID.",
                    "type": "object"
                }
            }
        },
        "models.UpdateResult": {
            "type": "object",
            "properties": {
                "matchedCount": {
                    "description": "The number of documents matched by the filter.",
                    "type": "integer"
                },
                "modifiedCount": {
                    "description": "The number of documents modified by the operation.",
                    "type": "integer"
                },
                "upsertedCount": {
                    "description": "The number of documents upserted by the operation.",
                    "type": "integer"
                },
                "upsertedID": {
                    "description": "The _id field of the upserted document, or nil if no upsert was done.",
                    "type": "object"
                }
            }
        },
        "models.User": {
            "type": "object",
            "properties": {
                "created_at": {
                    "type": "string"
                },
                "deleted_at": {
                    "type": "string"
                },
                "email": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "user_id": {
                    "type": "string"
                }
            }
        }
    }
}